<section class="navs">
  <div class="new_breath">
    <button type="submit" class="btn btn_new">New Deep Breathing Benefit</button>
  </div>
  <div class="breath-benefit">
    <a href="/benefits"><button type="submit" class="btn btn_new">Deep Breathing Benefits</button></a>
  </div>
</section>

{{#each breaths as |breath|}}
  <div class="card" id="cardid">
    <div class="card_header">
      <h3>{{breath.relax_deep_breath_name}}</h3>
      <p>User: {{breath.user.name}}</p>
    </div>
    <div class="card_body">

      <p>{{breath.relax_deep_breath_description}}</p>
      
    </div>
  </div>
{{/each}}

<script src="/js/breath.js"></script>